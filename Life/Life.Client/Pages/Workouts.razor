@page "/Workouts"
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Workouts</title>
    <link rel="stylesheet" href="~/css/styles.css">
</head>
<body>
<div class="workout-container">
    <h1>Workouts</h1>

    <!-- Workout Item -->
    <div class="workout-item">
        <div class="workout-image">
            <img src="workout_image.jpg" alt="Workout Image" />
        </div>
        <div class="workout-details">
            <h2>Workout Name</h2>
            <div class="workout-diagram">
                <img src="workout_diagram.png" alt="Workout Diagram" />
            </div>
            <div class="ingredients">
                <h3>Ingredients</h3>
                <ul>
                    <li>Ingredient 1</li>
                    <li>Ingredient 2</li>
                    <li>Ingredient 3</li>
                </ul>
            </div>
            <div class="exercises">
                <h3>Exercises</h3>
                <ul>
                    <li>Exercise 1</li>
                    <li>Exercise 2</li>
                    <li>Exercise 3</li>
                </ul>
            </div>
        </div>
    </div>
</div>
</body>

